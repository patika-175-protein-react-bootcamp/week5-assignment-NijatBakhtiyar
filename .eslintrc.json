{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "max-len": "off",
    "object-curly-spacing": "off",
    "linebreak-style": 0,
    "require-jsdoc": 0,
    "indent": "off",
    "react/prop-types": 0
  }
}
